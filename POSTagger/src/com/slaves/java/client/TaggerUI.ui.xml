<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	
	</ui:style>
	<g:HTMLPanel>
		<div class="main">
		    <div ui:field="sidebar">
		        <g:CheckBox ui:field="CCCheckBox">conjunctions</g:CheckBox><br/>
		        <g:CheckBox ui:field="CDCheckBox">cardinals</g:CheckBox><br/>
		        <g:CheckBox ui:field="DTCheckBox">determiners</g:CheckBox><br/>
		        <g:CheckBox ui:field="EXCheckBox">existential &quot;there&quot;</g:CheckBox><br/>
		        <g:CheckBox ui:field="FWCheckBox">foreign words</g:CheckBox><br/>
		        <g:CheckBox ui:field="INCheckBox">prepositions</g:CheckBox><br/>
		        <g:CheckBox ui:field="JJCheckBox">adjectives</g:CheckBox><br/>
		        <g:CheckBox ui:field="LSCheckBox">list markers</g:CheckBox><br/>
		        <g:CheckBox ui:field="NNCheckBox">nouns</g:CheckBox><br/>
		        <g:CheckBox ui:field="POSCheckBox">possession indicators</g:CheckBox><br/>
		        <g:CheckBox ui:field="RBCheckBox">adverbs</g:CheckBox><br/>
		        <g:CheckBox ui:field="RPCheckBox">particles</g:CheckBox><br/>
		        <g:CheckBox ui:field="SYMCheckBox">symbols</g:CheckBox><br/>
		        <g:CheckBox ui:field="UHCheckBox">interjections</g:CheckBox><br/>
		        <g:CheckBox ui:field="VBCheckBox">verbs</g:CheckBox><br/>
		        <g:CheckBox ui:field="WHCheckBox">&quot;wh-&quot; pronouns</g:CheckBox><br/>
		    </div>
		    <div id="textFrame">
				<g:TextArea ui:field="text"/>
			</div>
			<div ui:field="taggedTextArea"></div>
			<div id="submitButton">
				<g:Button ui:field="submit">Tag my text</g:Button>
				<g:Button ui:field="returnBtn">Back to text entry</g:Button>
			</div>
		</div>
	</g:HTMLPanel>
</ui:UiBinder>